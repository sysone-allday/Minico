<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/21.0.7" xmlns:fx="http://javafx.com/fxml/1" fx:controller="allday.minico.controller.member.CharacterSelectController">
    <children>
        <Button fx:id="backToSignUpButton" layoutX="29.0" layoutY="33.0" mnemonicParsing="false" onAction="#backToSignUp" text="뒤로가기" />
        <Button fx:id="selectMaleMinimiButton" layoutX="395.0" layoutY="55.0" mnemonicParsing="false" onAction="#selectMaleMinimi" prefHeight="119.0" prefWidth="142.0" text="남자 미니미" />
        <Button fx:id="selectFemaleMinimiButton" layoutX="392.0" layoutY="200.0" mnemonicParsing="false" onAction="#selectFemaleMinimi" prefHeight="126.0" prefWidth="148.0" text="여자 미니미" />
        <ImageView fx:id="ViewSelectedCharacter" fitHeight="201.0" fitWidth="268.0" layoutX="61.0" layoutY="90.0" pickOnBounds="true" preserveRatio="true" />
        <Button fx:id="characterSelectConfirmButton" layoutX="274.0" layoutY="326.0" mnemonicParsing="false" onAction="#characterSelectConfirm" text="확인" />
    </children>
</AnchorPane>
